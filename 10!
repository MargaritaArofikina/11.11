//Напишите процедуру, удаляющую из динамически выделенного массива все четные значения.
//По оконрчании работы процедуры количество выделенной памяти должно уменьшиться.


#include <iostream>
using namespace std;

void del(int n, unsigned long long * p_darr) {

	for (int i = 0; i < n; i++){
		int j = 0;
		if (p_darr[i] % 2 != 0) {
			unsigned long long p_darr2[j] = p_darr[i];
		}
			j++;
		}
	}
}


int main(){

	int n;
	unsigned long long p_darr;

	del(n, p_darr)


	delete[] p_darr;

	return 0;
}
