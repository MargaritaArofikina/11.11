// Напишите 2 функции, находящие приближенное значение квадратного и кубического корня. 

#include <iostream>
using namespace std;

float root2 (float n, float i) {    //квадратный корень                   // fatal error LNK1120: 1 unresolved externals

	for (i = 1.0; i <= n; i = i + 0.1){
		if ((n - (i*i)) < 0.1 && (n - (i*i)) > -0.1) break;
	}

	return i;
}


float root3 (float m, float j) {    //кубический корень

	for (j = 1.0; j <= m; j = j + 0.1){
		if ((m - (j*j*j)) < 0.2 && (m - (j*j*j)) > -0.2) break;
	}

	return j;
}



#include <iostream>
using namespace std;

double root2(double n) {    //квадратный корень  

	double m;                         //(24): warning C4244: '=' : conversion from '__int64' to 'double', possible loss of data
	double a = 0.00001;
	for (double i = 0;; i += a){
		if (((i*i) <= n) && (((i + a)*(i + a)) >= n)) {
			m = i;
			break;
		}
	}
	m *= (1 / a);
	long long s = (long long) m + 1;
	int e = (s % 10);
	if (e >= 5){
		s -= e;
		s += 10;
	}
	else {
		s -= e;
	}
	m = (s / 10);
	m *= (a * 10);

	return m;

}
