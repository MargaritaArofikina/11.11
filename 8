// Напишите 2 функции, находящие приближенное значение квадратного и кубического корня. 

#include <iostream>
using namespace std;

double root2(double n) {    //квадратный корень  

	double m;                         
	double a = 0.00001;
	for (double i = 0; ; i += a){
		if (((i*i) <= n) && (((i + a)*(i + a)) >= n)) {
			m = i;
			break;
		}
	}
	m *= (1 / a);
	long long s = (long long)m;
	int e = (s % 10);
	if (e >= 5){
		s -= e;
		s += 10;
	}
	else {
		s -= e;
	}
	m = (s / 10);
	m *= (a * 10);

	return m;

}



double root3(double n) {    //кубический корень  

	double m;
	double a = 0.00001;
	for (double i = 0; ; i += a){
		if (((i*i*i) <= n) && (((i + a)*(i + a)*(i + a)) >= n)) {
			m = i;
			break;
		}
	}
	m *= (1 / a);
	long long s = (long long)m;
	int e = (s % 10);
	if (e >= 5){
		s -= e;
		s += 10;
	}
	else {
		s -= e;
	}
	m = (s / 10);
	m *= (a * 10);

	return m;

}




int main(){
	double n;
	cin >> n;
	cout << root2(n) << "  " << root3(n); 

	return 0;
}
