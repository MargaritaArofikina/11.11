# 1 Напишите программу, вычисляющую сумму нечетных цифр целого числа

#include <iostream>
using namespace std;

int main() {

	int a,b,c,i,sum;
	cin >> a;
	sum = 0;
	b = a;
	for (i = 10; b >= 1 ; i *= 10) {
		b = a / i;
	}
	if ((b % 2 )!= 0) sum = b;

	for (int j = 100; j <= i ; j *= 10) {
		c = (a /( i / j)) % (10);
		if ((c % 2 )!= 0) sum += c;
	}

	cout << sum;



	return 0;
}

