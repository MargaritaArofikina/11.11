//Напишите программу, находящую сумму простых элементов массива.

#include <iostream>
#include <vector>
using namespace std;

int main() {

	int n,sum;

	cin >> n;

	vector <int> a(n);

	sum = 0;
	for (int i = 0; i < n; i++) {
		cin >> a[i];
		
		if (a[i] == 2 || a[i] == 3) {
			sum += a[i];
		}
		else {
			
			 
			for (int j = 2; j <= (a[i] / 2); j++) {
				if (a[i] % j == 0) {
					break;
				}
				if ((a[i] / 2) == j) {
					sum += a[i];
				}
				
			}
		}
	}

	cout << sum;




	return 0;
}
